			"name": "Registrar Usuário",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"//11 9 1234-5678\r",
							"var ddd = _.random(10, 99)\r",
							"var numeroADicional = _.random(1,9)\r",
							"var primeiros4Digitos = _.random(1000,9999)\r",
							"var ultimos4Digitos = _.random(1000,9999)\r",
							"\r",
							"numeroTelefone = ddd +\" \"+ numeroADicional +\" \"+ primeiros4Digitos +\"-\"+ ultimos4Digitos\r",
							"\r",
							"pm.enviroment.set(\"numeroTelefone\", numeroTelefone)"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Validar sucesso da requisição\", function(){\r",
							"    pm.response.to.have.status(201);\r",
							"    pm.response.to.be.withBody;\r",
							"    pm.response.to.have.jsonBody(\"token\");\r",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"nome\": \"{{$randomFullName}}\",\r\n  \"email\": \"{{$randomEmail}}\",\r\n  \"telefone\": \"{{numeroTelefone}}\",\r\n  \"senha\": \"{{$randomPassword}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{URLBASE}}/usuario",
					"host": [
						"{{URLBASE}}"
					],
					"path": [
						"usuario"
					]
				}
			},
			"response": []
		}
